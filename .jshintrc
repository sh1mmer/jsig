{
    "maxdepth": 4,
    "maxstatements": 200,
    "maxcomplexity": 12,
    "maxlen": 80,
    "maxparams": 4,

    "bitwise": false,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "latedef": "func",
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "shadow": "outer",
    "trailing": true,
    "undef": true,
    "unused": true,

    "camelcase": true,
    "expr": true,
    "indent": 4,
    "globalstrict": true,
    "newcap": false,
    "quotmark": "single",
    "strict": true,

    "browser": false,
    "esnext": true,
    "devel": false,
    "node": false,
    "nonstandard": false,

    "predef": {
        "require": false,
        "module": false,
        "__dirname": false,
        "__filename": false,
        "console": true
    }
}

